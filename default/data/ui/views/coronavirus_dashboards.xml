<dashboard theme="dark">
  <label>Coronavirus Dashboards</label>
  <search id="base">
    <query>index="covid_19" sourcetype="covid19-report" host="jh-covid-reports" | fields _time Confirmed Deaths Recovered Province_State Country_Region</query>
    <earliest>-30d@d</earliest>
    <latest>now</latest>
    <sampleRatio>1</sampleRatio>
  </search>
  <row>
    <panel>
      <title>Total Cases Worldwide</title>
      <single>
        <search base="base">
          <query>
| bin _time span=1d 
| stats sum(Confirmed) as confirmed_total by _time
| table _time confirmed_total
          </query>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trellis.splitBy">_aggregation</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">percent</option>
        <option name="trendInterval">-7d</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <title>Top Death Rate</title>
      <single>
        <search base="base">
          <query>search Country_Region="Iran"
| bin _time span=1d 
| fillnull value=0 
| replace "0" WITH "" IN Province_State 
| stats max(Confirmed) as confirmed_count, max(Deaths) as death_count, max(Recovered) as recovered_count by _time, Country_Region
| search confirmed_count &gt; 100 AND death_count &gt; 0 AND recovered_count &gt; 0
| eval deathratio = round(death_count/confirmed_count*100,2)
| eval recoveredratio = round(recovered_count/confirmed_count*100,2)
| table _time deathratio</query>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trellis.splitBy">_aggregation</option>
        <option name="trendColorInterpretation">inverse</option>
        <option name="trendDisplayMode">percent</option>
        <option name="trendInterval">-7d</option>
        <option name="underLabel">Minimum of 100 Cases</option>
        <option name="unit">%</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <title>Lowest Recovery Rate</title>
      <single>
        <search base="base">
          <query>| search Province_State="Diamond Princess cruise ship"
| bin _time span=1d 
| fillnull value=0 
| replace "0" WITH "" IN Province_State 
| stats max(Confirmed) as confirmed_count, max(Deaths) as death_count, max(Recovered) as recovered_count by _time, Province_State, Country_Region
| search confirmed_count &gt; 100 AND death_count &gt; 0 AND recovered_count &gt; 0
| eval deathratio = round(death_count/confirmed_count*100,2)
| eval recoveredratio = round(recovered_count/confirmed_count*100,2)
| table _time recoveredratio Province_State</query>
        </search>
        <option name="colorBy">trend</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="refresh.display">progressbar</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trellis.splitBy">_aggregation</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">percent</option>
        <option name="trendInterval">-7d</option>
        <option name="underLabel">Minimum of 100 Cases</option>
        <option name="unit">%</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <title>Last 7 Days Report</title>
      <table>
        <title>Top 10 Regions</title>
        <search base="base">
          <query>
| eval Country_State = if(isnull(Province_State), Country_Region,  Country_Region + " - " + Province_State)
| lookup world_coords.csv Country_State
| fillnull value=0 
| replace "0" WITH "" IN Province_State 
| stats max(Confirmed) as total_confirmed, max(Deaths) as total_death, max(Recovered) as total_recovered by Province_State, Country_Region
| eval deathratio = round(total_death/total_confirmed,2)
| eval recoveredratio = round(total_recovered/total_confirmed,2)
| rename total_confirmed as "Total Confirmed Cases", total_death as "Total Death Count", total_recovered as "Total Recovered Count"
| sort -"Total Confirmed Cases"</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="confirmed_count">
          <colorPalette type="minMidMax" maxColor="#DC4E41" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="color" field="death_count">
          <colorPalette type="minMidMax" maxColor="#FFFFFF" minColor="#53A051"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
        <format type="color" field="deathratio">
          <colorPalette type="minMidMax" maxColor="#AF575A" midColor="#FFFFFF" minColor="#62B3B2"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="50"></scale>
        </format>
        <format type="color" field="recoveredratio">
          <colorPalette type="minMidMax" maxColor="#4FA484" midColor="#FFFFFF" minColor="#F8BE34"></colorPalette>
          <scale type="minMidMax" midType="percentile" midValue="50"></scale>
        </format>
      </table>
    </panel>
  </row>
