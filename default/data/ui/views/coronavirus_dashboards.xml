  <row>
    <panel>
      <title>Choropleth Map</title>
      <map>
        <title>Colored by Number of Cases</title>
        <search base="base">
          <query>
| eval Country_State = if(isnull(Province_State), Country_Region,  Country_Region + " - " + Province_State)
| lookup world_coords.csv Country_State
| rename Lat as latitude, Long as longitude
| fillnull value=0
| replace "Mainland China" WITH "China" IN Country_Region
| stats max(Confirmed) as total_confirmed by Country_Region | geom geo_countries featureIdField=Country_Region | eval total_confirmed = log(total_confirmed)</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.choroplethLayer.colorBins">9</option>
